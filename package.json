{
  "name": "@farm-js/react-goat",
  "version": "1.0.1",
  "description": "How did it get there?",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts",
      "source": "./src/index.ts"
    },
    "./ts/*": {
      "types": "./dist/types/index.d.ts",
      "source": "./src"
    },
    "./esm/*": "./dist/esm/*",
    "./cjs/*": "./dist/cjs/*"
  },
  "directories": {
    "ts": "src",
    "esm": "dist/esm",
    "cjs": "dist/cjs"
  },
  "scripts": {
    "build:cjs": "tsc -p tsconfig.build.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "jest && rm -rf dist/* && npm run build:cjs && npm run build:esm && typedoc",
    "doc": "typedoc",
    "test": "jest --config ./jest.config.ts",
    "test:e2e": "jest --config ./jest.config.e2e.ts --forceExit --verbose --detectOpenHandles --coverage --runInBand"
  },
  "author": "joneldiablo",
  "license": "ISC",
  "dependencies": {
    "@floating-ui/react": "^0.27.5",
    "@splidejs/react-splide": "^0.7.12",
    "css-element-queries": "^1.2.3",
    "dbl-utils": "^1.0.8",
    "html-react-parser": "^5.2.2",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icomoon": "^2.6.1",
    "react-router": "^7.2.0",
    "react-router-dom": "^7.2.0",
    "swiper": "^11.2.5",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/change-case": "^2.3.5",
    "@types/chroma-js": "^2.4.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/flat": "^5.0.5",
    "@types/jest": "^29.5.14",
    "@types/json2csv": "^5.0.7",
    "@types/lodash": "^4.17.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.7.9",
    "@types/numeral": "^2.0.5",
    "@types/prop-types": "^15.7.14",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/uuid": "^10.0.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.11",
    "typescript": "^5.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joneldiablo/goat/react-goat.git"
  },
  "keywords": [
    "NodeJS",
    "TypeScript",
    "JavaScript",
    "ESM",
    "CommonJS",
    "frontend",
    "react"
  ]
}